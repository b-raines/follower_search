<h1 id = "logo">Follower Trends</h1>
<% if current_user %>
	<div id = "signin">
		<span class = "status">
			Signed in as <em><%= current_user.name %></em>
		</span>
		<%= link_to "Sign out", signout_path %>
	</div>
<% else %>
	<div id = "signin">
		<%= link_to  "Sign in with Twitter", "/auth/twitter" %>
	</div>
<% end %>
